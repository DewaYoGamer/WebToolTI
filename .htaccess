Options -Indexes
RewriteEngine On

# Redirect requests to the root directory to index.php
RewriteRule ^$ /WebToolTI/index.html [L]

# Redirect requests to dashboard.php if it's a directory request
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^(.*)$ /WebToolTI/404.html [L,QSA]

# Redirect requests to dashboard.php for other cases
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /WebToolTI/404.html [L,QSA]